<Application x:Class="BrokFindTeacher.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:BrokFindTeacher"
             StartupUri="Windows/wBrok.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Paths/ResourcePaths.xaml"/>
                <ResourceDictionary Source="Resources/Colors/ResourceColors.xaml"/>
                <ResourceDictionary Source="Resources/Fonts/ResourceFonts.xaml"/>
                <ResourceDictionary Source="Styles/ResourceStyle_1.xaml"/>
                <ResourceDictionary Source="Styles/ResourceStyle_2.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
